class Blackhole {
  PImage blackhole;
  int x,y;
  void setup(int a,int b){
    blackhole= loadImage("img/blackhole.png");
    x=Util.getXbyCol(a);
    y=Util.getYbyRow(b);
  }
  void draw(){
    image(blackhole,x,y);
    if(isHit(fighter.x,fighter.y,51,51,this.x,this.y,50,50)){
        
      fighter.x=random(width);
      fighter.y=random(height);
      x= 50;
      y= 50;
    }
  }
}
